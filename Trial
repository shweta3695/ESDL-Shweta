#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
using namespace std;
int main()
{
  int a,b;
  cout<<"Simple program of addition"<<endl;
  cout<<"Enter value for a:"<<endl;
  cin>>a;
  cout<<"Enter value for b:"<<endl;
  cin>>b;
  int sum;
  sum=a+b;
  cout<<"a:"<<a<<endl;
  cout<<"b:"<<b<<endl;
  cout<<"sum:"<<sum<<endl;
  return 0;
  }
